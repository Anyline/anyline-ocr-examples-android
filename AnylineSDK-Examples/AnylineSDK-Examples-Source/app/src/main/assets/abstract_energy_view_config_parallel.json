{
  "camera": {
    "captureResolution": "1080p",
    "zoomGesture": true
  },
  "flash": {
    "mode": "auto",
    "alignment": "bottom_right",
    "imageOn": "flash_on",
    "imageOff": "flash_off",
    "imageAuto": "flash_auto"
  },
  "parallelViewPluginComposite": {
    "id": "parallel_meter_barcode",
    "cancelOnResult": false,
    "viewPlugins": [
      {
        "viewPlugin": {
          "plugin": {
            "id": "barcode_id",
            "barcodePlugin": {
              "barcodeFormatOptions": [
                "AZTEC",
                "AZTEC_INVERSE",
                "BOOKLAND",
                "CODABAR",
                "CODE_11",
                "CODE_32",
                "CODE_39",
                "CODE_93",
                "CODE_128",
                "DATABAR",
                "COUPON",
                "DATA_MATRIX",
                "DISCRETE_2_5",
                "DOT_CODE",
                "EAN_8",
                "EAN_13",
                "GS1_QR_CODE",
                "GS1_128",
                "ITF",
                "ISSN_EAN",
                "ISBT_128",
                "KIX",
                "MATRIX_2_5",
                "MAXICODE",
                "MSI",
                "MICRO_QR",
                "MICRO_PDF",
                "ONE_D_INVERSE",
                "PDF_417",
                "POST_UK",
                "QR_CODE",
                "QR_INVERSE",
                "RSS_14",
                "RSS_EXPANDED",
                "TRIOPTIC",
                "UPC_A",
                "UPC_E",
                "UPU_FICS",
                "USPS_4CB",
                "US_PLANET",
                "US_POSTNET"
              ]
            }
          },
          "cutoutConfig": {
            "style": "rect",
            "alignment": "top",
            "strokeWidth": 2,
            "strokeColor": "00FFFFFF",
            "outerColor": "FFFFFF",
            "outerAlpha": 0,
            "feedbackStrokeColor": "00FF33FF"
          },
          "cancelOnResult": true,
          "scanFeedback": {
            "style": "rect",
            "strokeColor": "0099FF",
            "fillColor": "220099FF",
            "beepOnResult": true,
            "vibrateOnResult": false,
            "strokeWidth": 2
          }
        }
      },
      {
        "viewPlugin": {
          "plugin": {
            "id": "METER_ID",
            "meterPlugin": {
            }
          },
          "cutoutConfig": {
            "style": "rect",
            "alignment": "top",
            "strokeWidth": 2,
            "strokeColor": "FFFFFF",
            "cornerRadius": 4,
            "outerColor": "000000",
            "outerAlpha": 0.5,
            "feedbackStrokeColor": "0099FF",
            "offset": {
              "x": 0,
              "y": 120
            }
          },
          "scanFeedback": {
            "style": "CONTOUR_RECT",
            "strokeColor": "0099FF",
            "fillColor": "220099FF",
            "blinkOnResult": true,
            "beepOnResult": true,
            "vibrateOnResult": true
          },
          "cancelOnResult": true
        }
      }
    ]
  }
}
